<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name = 'viewport' content = 'width=device-width, initial-scale=1, user-scalable=0'>
    <meta name='apple-mobile-web-app-capable' content='yes' />
    <meta name='apple-mobile-web-app-status-bar-style' content='black' />
    <title>Avid5</title>
    <link rel="stylesheet" href="~/css/Site.css" />
    <link rel="stylesheet" href="~/css/nouislider.fox.css" />
    <link rel="SHORTCUT ICON" href="@Url.Content("~/favicon.ico")" />
    <link rel="apple-touch-icon" href="~/apple-touch-icon.png" />
    <link rel="icon" href="~/Avid5-192.png" sizes="192x192" />
</head>
<body>
    @RenderBody()

    <script src='~/lib/jquery/dist/jquery.min.js' type='text/javascript'></script>
    <script src='~/js/modernizr-2.8.3.js' type='text/javascript'></script>
    <script src='~/js/touch-emulator.js' type='text/javascript'></script>
    <script src='~/js/jquery.nouislider.js' type='text/javascript'></script>
    <script src='~/js/jquery.easing.min.js' type='text/javascript'></script>
    <script src='~/js/hammer.js' type='text/javascript'></script>
    @Html.Raw(Render.Script("functions"))
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
